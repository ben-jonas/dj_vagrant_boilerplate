server {
    listen 80;
    server_name 192.168.33.10;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /vagrant/proj/myproject;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/vagrant/myproject.sock;
    }
}